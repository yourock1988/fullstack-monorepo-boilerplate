{
  "name": "@monorepo/backend",
  "version": "0.0.1",
  "description": "backend",
  "author": "YouROCK",
  "license": "MIT",
  "main": "./src/server.js",
  "scripts": {
    "dev": "nodemon ./dev.js",
    "look": "npm run build:dev && nodemon ../../build/server/http.js",
    "start": "npm run build:prod && node ../../dist/server/http.js",
    "build:dev": "babel -d ../../build/server ./src --delete-dir-on-start -s",
    "build:prod": "babel -d ../../dist/server ./src --delete-dir-on-start",
    "lint:js": "eslint ./src",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "eslintConfig": {
    "extends": [
      "../../packages/eslint-config/node.json"
    ]
  },
  "babel": {
    "extends": "../../packages/babel-config/node.json"
  },
  "dependencies": {
    "express": "^4.21.2"
  }
}
